{
  "name": "falling-leaves",
  "main": "./lib/falling-leaves",
  "version": "1.6.0",
  "description": "Falling Leaves in your editor. Happy Fall!",
  "keywords": [
    "fall",
    "leaves",
    "snow",
    "october",
    "colorful"
  ],
  "homepage": "https://atom.io/packages/falling-leaves",
  "repository": "https://github.com/iamsenthilprabu/falling-leaves",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "configSchema": {
    "autoToggle": {
      "type": "boolean",
      "default": true,
      "title": "Auto Toggle",
      "description": "Toggle Falling Leaves on start.",
      "order": 1
    },
    "leaves": {
      "type": "object",
      "title": "Leaves Configuration",
      "description": "Settings for Leaves.",
      "order": 2,
      "properties": {
        "count": {
          "type": "integer",
          "default": 15,
          "minimum": 3,
          "maximum": 200,
          "title": "Number of Leaves",
          "order": 2
        },
        "speed": {
          "type": "integer",
          "default": 2,
          "minimum": 1,
          "maximum": 20,
          "title": "Speed",
          "order": 3
        },
        "size": {
          "type": "integer",
          "default": 40,
          "minimum": 1,
          "maximum": 100,
          "title": "Size",
          "order": 4
        }
      }
    },
    "flakes": {
      "type": "object",
      "title": "Flakes Configuration",
      "description": "Advanced settings for Flakes.",
      "order": 3,
      "properties": {
        "include": {
          "type": "boolean",
          "default": true,
          "order": 1,
          "title": "Inlcude Flakes"
        },
        "count": {
          "type": "integer",
          "default": 35,
          "minimum": 1,
          "maximum": 500,
          "order": 2,
          "title": "Flakes Count"
        },
        "minSize": {
          "type": "integer",
          "default": 1,
          "minimum": 1,
          "maximum": 5,
          "order": 3,
          "title": "Minimum Size of the flakes"
        },
        "maxSize": {
          "type": "integer",
          "default": 3,
          "minimum": 1,
          "maximum": 5,
          "order": 4,
          "title": "Maximum Size of the flakes"
        }
      }
    }
  },
  "dependencies": {
    "jquery": "^3.0.0"
  }
}
